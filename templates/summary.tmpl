{{template "header" .}}
<a class="noprint" href="/"><img width="32px" height="32px" src="https://www.rawshorts.com/freeicons/wp-content/uploads/2017/01/blue_repicthousebase_1484336386-1.png"></a>
<a class="noprint" href="/erase"><img width="32px" height="32px" src="https://cdn-icons-png.flaticon.com/512/167/167722.png"></a>
<div style="float:right" class="progress">
<div class="bar"></div >
<div class="percent">{{.progress}}%</div >
</div>

<table>
    {{range .results}}
    <tr>
        <td width="10%"><img width="50%" height="50%" src="{{.ImageURL}}"/></td>
        <td><a id="{{.Id}}" href="/a/{{.Id}}">{{.Title}}</a></td>
        <td><a id="{{.Id}}" href="/c/{{.Id}}/{{.ChoiceI}}">{{.Choice}}</a></td>
        <td><a id="{{.Id}}" href="/c/{{.Id}}/{{.ChoiceI}}">{{.Answer}}</a></td>
    </tr>
    {{end}}
</table>